{
    "name": "heimrichhannot/contao-tiny-slider-bundle",
    "description": "Tiny slider, a vanilla javascript content slider for contao.",
    "keywords": [
        "contao",
        "carousel",
        "slider",
        "tiny slider",
        "vanilla javascript"
    ],
    "type": "contao-bundle",
    "homepage": "https://github.com/heimrichhannot/contao-tiny-slider-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Heimrich & Hannot",
            "email": "digitales@heimrich-hannot.de",
            "homepage": "http://www.heimrich-hannot.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/heimrichhannot/contao-tiny-slider-bundle/issues"
    },
    "require": {
        "php": "^7.1|^8.0",
        "contao/core-bundle": "^4.9",
        "heimrichhannot/contao-multi-column-editor-bundle": "^2.0",
        "matthiasmullie/minify": "~1.3",
        "heimrichhannot/contao-utils-bundle": "^2.3",
        "symfony/event-dispatcher-contracts": "^1.0 || ^2.0 || ^3.0"
    },
    "require-dev": {
        "contao/test-case": "^1.1",
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.2",
        "phpunit/phpunit": ">=6.0 <6.5",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "php-coveralls/php-coveralls": "^2.0",
        "symfony/phpunit-bridge": "^3.2"
    },
    "replace": {
        "heimrichhannot/contao-tiny-slider-list-bundle": "*"
    },
    "autoload": {
        "psr-4": {
            "HeimrichHannot\\TinySliderBundle\\": "src/"
        },
        "classmap": [
            "src/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HeimrichHannot\\TinySliderBundle\\Test\\": "tests/"
        }
    },
    "extra": {
        "contao-manager-plugin": "HeimrichHannot\\TinySliderBundle\\ContaoManager\\Plugin",
        "foxy": true
    }
}
